<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >
    
    <Row>
        <Key android:codes="113" android:keyLabel="1^" android:keyEdgeFlags="left"/>
        <Key android:codes="119" android:keyLabel="2^"/>
        <Key android:codes="101" android:keyLabel="3^"/>
        <Key android:codes="114" android:keyLabel="4^"/>
        <Key android:codes="116" android:keyLabel="5^"/>
        <Key android:codes="121" android:keyLabel="6^"/>
        <Key android:codes="117" android:keyLabel="7^"/>
        <Key android:codes="105" android:keyLabel="8^"/>
        <Key android:codes="111" android:keyLabel="9^"/>
        <Key android:codes="112" android:keyLabel="0^" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="97" android:keyLabel="1-" android:keyEdgeFlags="left"/>
        <Key android:codes="115" android:keyLabel="2-"/>
        <Key android:codes="100" android:keyLabel="3-"/>
        <Key android:codes="102" android:keyLabel="4-"/>
        <Key android:codes="103" android:keyLabel="5-"/>
        <Key android:codes="104" android:keyLabel="6-"/>
        <Key android:codes="106" android:keyLabel="7-"/>
        <Key android:codes="107" android:keyLabel="8-"/>
        <Key android:codes="108" android:keyLabel="9-"/>
        <Key android:codes="59" android:keyLabel="0-" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="122" android:keyLabel="1v" android:keyEdgeFlags="left"/>
        <Key android:codes="120" android:keyLabel="2v"/>
        <Key android:codes="99" android:keyLabel="3v"/>
        <Key android:codes="118" android:keyLabel="4v"/>
        <Key android:codes="98" android:keyLabel="5v"/>
        <Key android:codes="110" android:keyLabel="6v"/>
        <Key android:codes="109" android:keyLabel="7v"/>
        <Key android:codes="44" android:keyLabel="8v"/>
        <Key android:codes="46" android:keyLabel="9v"/>
        <Key android:codes="47" android:keyLabel="0v" android:keyEdgeFlags="right"/>
    </Row>


    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-3"  android:keyIcon="@drawable/sym_keyboard_done" android:iconPreview="@drawable/sym_keyboard_feedback_done" android:isModifier="true" android:keyWidth="15%p" android:keyEdgeFlags="left"/>   
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:iconPreview="@drawable/sym_keyboard_feedback_shift" android:isModifier="true" 
                android:keyWidth="15%p" 
                android:isSticky="true"/>             
        <Key android:codes="-9" android:keyIcon="@drawable/sw" android:isModifier="true" android:keyWidth="15%p" android:iconPreview="@drawable/sw_l"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space_lime" android:iconPreview="@drawable/sym_keyboard_feedback_space"  android:keyWidth="25%p" android:isRepeatable="true"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:iconPreview="@drawable/sym_keyboard_feedback_delete" android:isModifier="true" 
                android:keyWidth="15%p" android:isRepeatable="true"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:iconPreview="@drawable/sym_keyboard_feedback_return" android:isModifier="true" android:keyWidth="15%p" android:keyEdgeFlags="right"/>
    </Row>
    
</Keyboard>
    